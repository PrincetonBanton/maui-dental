    <FlyoutItem Title="User" Icon="dashboard_icon.png">
        <Tab Title="User">
            <ShellContent ContentTemplate="{DataTemplate pages:UserPage}" Route="UserPage" />
        </Tab>
    </FlyoutItem>

</Shell>
